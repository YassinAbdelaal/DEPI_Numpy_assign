    {
      "cell_type": "code",
      "source": [
        "arr_5a= np.zeros((3, 3), dtype=int)\n",
        "for i in range(3):\n",
        "    for j in range(3):\n",
        "        arr_5a[i, j] = i * j\n",
        "print(arr_5a)"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
